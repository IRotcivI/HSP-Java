<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="appli.meditrack.ListeProduitsController">
    <TableView fx:id="tableProduits" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
        <columns>
            <TableColumn text="Libellé" fx:id="colLibelle" prefWidth="150" />
            <TableColumn text="Description" fx:id="colDescription" prefWidth="300" />
            <TableColumn text="Niveau de Dangerosité" fx:id="colNiveauDangerosite" prefWidth="200" />
        </columns>
    </TableView>
</AnchorPane>
