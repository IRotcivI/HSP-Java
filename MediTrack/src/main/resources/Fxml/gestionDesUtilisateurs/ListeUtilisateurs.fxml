<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="appli.meditrack.ListeUtilisateursController">
    <TableView fx:id="tableUtilisateur" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
        <columns>
            <TableColumn text="Nom" fx:id="colNom" prefWidth="150" />
            <TableColumn text="Prenom" fx:id="colPrenom" prefWidth="300" />
            <TableColumn text="Email" fx:id="colEmail" prefWidth="200" />
        </columns>
    </TableView>
</AnchorPane>
